' This enum contains the field names that are assigned by ArcObjects when performing
' a zonal statistics calculation; It needs to be kept in sync with StatisticsTypeString

Public Enum StatisticsFieldName

    MAJORITY = 1
    MAX = 2
    MEAN = 3
    MEDIAN = 4
    MIN = 5
    MINORITY = 6
    RANGE = 7
    STD = 8
    SUM = 9
    VARIETY = 10

End Enum
