' This enum contains statistic type strings used by ArcObjects when performing
' a zonal statistics calculation; It needs to be kept in sync with StatisticsFieldName

Public Enum StatisticsTypeString

    MAJORITY = 1
    MAXIMUM = 2
    MEAN = 3
    MEDIAN = 4
    MINIMUM = 5
    MINORITY = 6
    RANGE = 7
    STD = 8
    SUM = 9
    VARIETY = 10

End Enum
